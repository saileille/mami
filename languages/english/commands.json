    ],
    "sub_commands": {
      "category": {
        "description": "Category configuration and settings.",
        "names": [
          "category"
        ],
        "sub_commands": {
          "command_rules": {
            "description": "Manage who is allowed to use commands in the category.",
            "names": [
              "command_rules"
            ],
            "sub_commands": {
              "add": {
                "description": "Add command rule(s).",
                "names": [
                  "add"
                ],
                "sub_commands": {
                  "exclusionary": {
                    "arguments": {
                      "commands": {
                        "description": "To which commands do you want to add the rule? (Separate the commands with a space, do not use the prefix.)",
                        "name": "Commands"
                      },
                      "rule": {
                        "description": "Give a member, role or server permission to which you want the rule to apply.",
                        "name": "Rule"
                      }
                    },
                    "description": "Add an exclusionary rule.",
                    "names": [
                      "exclusionary"
                    ]
                  },
                  "inclusionary": {
                    "arguments": {
                      "commands": {
                        "description": "To which commands do you want to add the rule? (Separate the commands with a space, do not use the prefix.)",
                        "name": "Commands"
                      },
                      "rule": {
                        "description": "Give a member, role or server permission to which you want the rule to apply.",
                        "name": "Rule"
                      }
                    },
                    "description": "Add an inclusionary rule.",
                    "names": [
                      "inclusionary"
                    ]
                  }
                }
              },
              "display": {
                "arguments": {
                  "command": {
                    "description": "Insert the command whose command rules you want to see.",
                    "name": "Command"
                  }
                },
                "description": "Display given command's command rules.",
                "names": [
                  "display"
                ]
              },
              "remove": {
                "arguments": {
                  "commands": {
                    "description": "From which commands do you want to remove the rule? (Separate the commands with a space, do not use the prefix.)",
                    "name": "Commands"
                  },
                  "rule": {
                    "description": "Give a member, role or server permission to which you want the rule to no longer apply.",
                    "name": "Rule"
                  }
                },
                "description": "Remove command rule(s).",
                "names": [
                  "remove"
                ]
              }
            }
          },
          "language": {
