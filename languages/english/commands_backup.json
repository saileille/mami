  "settings": {
    "description": "Configuration and settings.",
    "names": [
      "settings"
    ],
    "sub_commands": {
      "guild": {
        "description": "Server configuration and settings.",
        "names": [
          "server",
          "guild"
        ],
        "sub_commands": {
          "checks": {
            "description": "Manage who is allowed to use commands in the server.",
            "names": [
              "command_rules"
            ],
            "sub_commands": {
              "add": {
                "description": "Add command rule(s).",
                "names": [
                  "add"
                ],
                "sub_commands": {
                  "allow": {
                    "arguments": {
                      "commands": {
                        "description": "To which commands do you want to add the rule? (Separate the commands with a space, do not use the prefix.)",
                        "name": "commands"
                      },
                      "rule": {
                        "description": "Give a member, role or guild permission to which you want the rule to apply.",
                        "name": "rule"
                      }
                    },
                    "description": "Add an allowing rule.",
                    "names": [
                      "allow"
                    ]
                  },
                  "deny": {
                    "arguments": {
                      "commands": {
                        "description": "To which commands do you want to add the rule? (Separate the commands with a space, do not use the prefix.)",
                        "name": "commands"
                      },
                      "rule": {
                        "description": "Give a member, role or guild permission to which you want the rule to apply.",
                        "name": "rule"
                      }
                    },
                    "description": "Add a denying rule.",
                    "names": [
                      "deny"
                    ]
                  }
                }
              },
              "remove": {
                "arguments": {
                  "commands": {
                    "description": "From which commands do you want to remove the rule? (Separate the commands with a space, do not use the prefix.)",
                    "name": "commands"
                  },
                  "rule": {
                    "description": "Give a member, role or guild permission to which you want the rule to no longer apply.",
                    "name": "rule"
                  }
                },
                "description": "Remove command rule(s)",
                "names": [
                  "remove"
                ]
              }
            }
          },
          "language": {
            "description": "Edit the server language.",
            "names": [
              "language"
            ],
            "sub_commands": {
              "set": {
                "arguments": {
                  "language": {
                    "description": "Which language do you want to set for the server?",
                    "name": "language"
                  }
                },
                "description": "Set the server language to a specific one.",
                "names": [
                  "set"
                ]
              }
            }
          },
          "shortcut": {
            "description": "Manage server shortcuts.",
            "names": [
              "shortcut",
              "sc"
            ],
            "sub_commands": {
              "add": {
                "arguments": {
                  "content": {
                    "description": "Enter the content for the new shortcut.",
                    "name": "content"
                  },
                  "name": {
                    "description": "Enter the name for the new shortcut.",
                    "name": "name"
                  }
                },
                "description": "Add a server shortcut.",
                "names": [
                  "add"
                ]
              },
              "delete": {
                "arguments": {
                  "name": {
                    "description": "Enter the name of the shortcut you wish to delete.",
                    "name": "name"
                  }
                },
                "description": "Delete an existing server shortcut.",
                "names": [
                  "delete",
                  "del"
                ]
              },
              "display": {
                "description": "Display all server shortcuts.",
                "names": [
                  "display"
                ]
              }
            }
          }
        }
      }
    }
  },
  "shortcut": {
    "arguments": {
      "name": {
        "description": "Which shortcut do you want to use?",
        "name": "name"
      }
    },
    "description": "Use existing shortcuts.",
    "names": [
      "shortcut",
      "sc"
    ]
  }
}
